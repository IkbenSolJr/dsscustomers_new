    <!-- list view Processing nghe -->
    <record model="ir.ui.view" id="view_dsscustomers_registerwork_tree">
      <field name="name">dsscustomers.registerwork.tree</field>
      <field name="model">dsscustomers.registerwork</field>
      <field name="arch" type="xml">
             <tree>
                  <field name="madangkynghe"/>
                  <field name="sinhvien_ids"/>
                  <field name="masokh"/>
                  <field name="visa"/>
                  <field name="phonekh" optional="show"/>
                  <field name="classroomwork_id"/>
                  <field name="coursework_id" optional="show"/>
                  <field name="classstatus" optional="show"/>
                  <field name="links_room" widget="url" optional="show"/>
                  <field name="score" optional="show"/>
                  <field name="feedback_training" optional="show"/>
                  <field name="ngaybatdau" optional="show"/>
                  <field name="ngaykethuc" optional="show"/> 
                  <field name="note" optional="show"/>
              </tree>
      </field>
    </record>

   <!-- Action-->
   <record model="ir.actions.act_window" id="action_dsscustomers_registerwork">
      <field name="name">Đăng ký học viên</field>
      <field name="res_model">dsscustomers.registerwork</field>
      <field name="view_mode">tree,form</field>
    </record>

<!-- Menu-->
 <menuitem name="Đăng ký học viên"
                id="dsscustomers_menu_work_6"
